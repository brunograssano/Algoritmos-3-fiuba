leNames: ''	package: 'BatallaNaval-Juego'!Exception subclass: #PosicionDelBoteFueraDelTablero	instanceVariableNames: ''	classVariableNames: ''	package: 'BatallaNaval-Juego'!Object subclass: #Tablero	instanceVariableNames: 'botes limiteMaximo limiteMinimo'	classVariableNames: ''	package: 'BatallaNaval-Juego'!!Tablero methodsFor: 'as yet unclassified' stamp: 'BrunoGrassano 6/29/2020 12:41'!estaOcupado: unaPosicion	^botes includesKey: unaPosicion! !!Tablero methodsFor: 'as yet unclassified' stamp: 'BrunoGrassano 6/29/2020 13:18'!agregarBote: unBote enPosicion: unaPosicion	(self contiene: unaPosicion ) ifTrue: [ botes at: unaPosicion put: unBote ]	ifFalse: [ PosicionDelBoteFueraDelTablero new signal ]! !!Tablero methodsFor: 'as yet unclassified' stamp: 'BrunoGrassano 6/29/2020 14:38'!atacarBoteEnPosicion: unaPosicion conAtaque: unAtaque 	botes at: unaPosicion ifPresent: [ :bote | 		bote recibirAtaque: unAtaque.		bote revisarSiSeHundio: self enPosicion: unaPosicion.	]	ifAbsent: [ NoHayUnBoteEnPosicion new signal ]! !!Tablero methodsFor: 'as yet unclassified' stamp: 'BrunoGrassano 6/29/2020 12:40'!hundirBoteEn: unaPosicion	botes removeKey: unaPosicion	ifAbsent: [ NoHayUnBoteEnPosicion new signal ]! !!Tablero methodsFor: 'as yet unclassified' stamp: 'BrunoGrassano 6/29/2020 12:29'!reiniciarTablero	botes removeAll! !!Tablero methodsFor: 'accessing' stamp: 'BrunoGrassano 6/29/2020 13:20'!contiene: unaPosicion	^ (limiteMinimo <= unaPosicion) & (unaPosicion <= limiteMaximo)! !!Tablero methodsFor: 'initialization' stamp: 'BrunoGrassano 6/29/2020 13:10'!initialize	botes := Dictionary new.	limiteMaximo := '8H'.	limiteMinimo := '1A'.! !Bote subclass: #Yate	instanceVariableNames: ''	classVariableNames: ''	package: 'BatallaNaval-Juego'!!Yate methodsFor: 'initialization' stamp: 'BrunoGrassano 6/29/2020 14:47'!initialize	vida := 90! !!Yate methodsFor: 'as yet unclassified' stamp: 'BrunoGrassano 6/29/2020 14:31'!recibirA