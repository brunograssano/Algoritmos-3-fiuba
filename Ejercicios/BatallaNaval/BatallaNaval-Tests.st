TestCase subclass: #TableroTestsParte1	instanceVariableNames: ''	classVariableNames: ''	package: 'BatallaNaval-Tests'!!TableroTestsParte1 methodsFor: 'tests' stamp: 'BrunoGrassano 6/29/2020 13:27'!testAlCrearElTableroDebeDeEstarVacio	|tablero|	tablero := Tablero new.		self assert: (tablero estaOcupado: '5F') equals: false.	self assert: (tablero estaOcupado: '6E') equals: false.	self assert: (tablero estaOcupado: '2B') equals: false.	self assert: (tablero estaOcupado: '1A') equals: false.! !!TableroTestsParte1 methodsFor: 'tests' stamp: 'BrunoGrassano 6/29/2020 13:21'!testIntentoAgregarUnBoteFueraDelTablero	|tablero bote|	bote := Bote new.	tablero := Tablero new.		self should: [tablero agregarBote: bote enPosicion: '0A'] raise: PosicionDelBoteFueraDelTablero ! !!TableroTestsParte1 methodsFor: 'tests' stamp: 'BrunoGrassano 6/29/2020 13:25'!testAgregoOtroBoteYLaPosicionDeberiaEStarOcupada	|bote tablero|	bote := Bote new.	tablero := Tablero new.		tablero agregarBote: bote enPosicion: '5F'.		self assert: (tablero estaOcupado: '5F') ! !!TableroTestsParte1 methodsFor: 'tests' stamp: 'BrunoGrassano 6/29/2020 14:53'!testAgregoVariosBarcosYReinicioElTablero	|bote yate lancha boteARemo tablero|	bote := Bote new.	yate := Yate new.	lancha := Lancha new.	boteARemo := BoteARemo new.		tablero := Tablero new.		tablero agregarBote: bote enPosicion: '1A'.	tablero agregarBote: yate enPosicion: '5A'.	tablero agregarBote: lancha enPosicion: '8D'.	tablero agregarBote: boteARemo enPosicion: '3G'.		tablero reiniciarTablero.		self assert: (tablero estaOcupado: '1A') equals: false.	self assert: (tablero estaOcupado: '5A') equals: false.	self assert: (tablero estaOcupado: '8D') equals: false.	self assert: (tablero estaOcupado: '3G') equals: false.	! !!TableroTestsParte1 methodsFor: 'tests' stamp: 'BrunoGrassano 6/29/2020 14:05'!testAgregoUnBoteEnUnaPosicionYPidoHundirloEnOtraDebeLanzarExcepcion	|tablero bote|	bote := Bote new.	tablero := Tablero new.	tablero agregarBote: bote enPosicion: '7D'.		self should: [tablero hundirBoteEn: '8D'] raise: NoHayUnBoteEnPosicion  ! !!TableroTestsParte1 methodsFor: 'tests' stamp: 'BrunoGrassano 6/29/2020 13:22'!testIntentoAgregarOtroBoteFueraDelTablero	|tablero bote|	bote := Bote new.	tablero := Tablero new.		self should: [tablero agregarBote: bote enPosicion: '9I'] raise: PosicionDelBoteFueraDelTablero ! !!TableroTestsParte1 methodsFor: 'tests' stamp: 'BrunoGrassano 6/29/2020 13:24'!testAgregoUnBoteYLaPosicionDeberiaEStarOcupada	|bote tablero|	bote := Bote new.	tablero := Tablero new.		tablero agregarBote: bote enPosicion: '1A'.		self assert: (tablero estaOcupado: '1A') ! !TestCase subclass: #TableroTestsParte2	instanceVariableNames: ''	classVariableNames: ''	package: 'BatallaNaval-Tests'!!TableroTestsParte2 methodsFor: 'tests' stamp: 'BrunoGrassano 6/29/2020 14:47'!testAgregoUnYateDeberiaHundirseConAtaquesDeMisil	|bote tablero|	bote := Yate new.	tablero := Tablero new.		tablero agregarBote: bote enPosicion: '7D'.	3 timesRepeat: [ tablero atacarBoteEnPosicion: '7D' conAtaque: (AtaqueMisil new) ].		self assert: (tablero estaOcupado: '7D') equals: false! !!TableroTestsParte2 methodsFor: 'tests' stamp: 'BrunoGrassano 6/29/2020 14:44'!testAgregoUnBoteARemoYDeberiaHundirseConUnAtaqueConvencional	|bote tablero|	bote := BoteARemo new.	tablero := Tablero new.		tablero agregarBote: bote enPosicion: '5F'.	tablero atacarBoteEnPosicion: '5F' conAtaque: (AtaqueConvencional new).		self assert: (tablero estaOcupado: '5F') equals: false! !!TableroTestsParte2 methodsFor: 'tests' stamp: 'BrunoGrassano 6/29/2020 14:46'!testAgregoUnYateNoDeberiaHundirseConAtaquesConvencionales	|bote tablero|	bote := Yate new.	tablero := Tablero new.		tablero agregarBote: bote enPosicion: '7D'.	5 timesRepeat: [ tablero atacarBoteEnPosicion: '7D' conAtaque: (AtaqueConvencional new) ].		self assert: (tablero estaOcupado: '7D')! !!TableroTestsParte2 methodsFor: 'tests' stamp: 'BrunoGrassano 6/29/2020 14:44'!testAgregoUnBoteARemoYDeberiaHundirseConUnAtaqueMisil	|bote tablero|	bote := BoteARemo new.	tablero := Tablero new.		tablero agregarBote: bote enPosicion: '5F'.	tablero atacarBoteEnPosicion: '5F' conAtaque: (AtaqueMisil new).		self assert: (tablero estaOcupado: '5F') equals: false! !