TestCase subclass: #PruebasBikeRental	instanceVariableNames: ''	classVariableNames: ''	package: 'BikeRental-Tests'!!PruebasBikeRental methodsFor: 'tests' stamp: 'BrunoGrassano 6/27/2020 17:01'!test01PruebasBicicleta1| unaBici unAlquiler unCliente |unaBici := Bici conPrecio: 10.unAlquiler := Alquiler crearAlquilerDiario: unaBici.unCliente := Cliente new.unCliente agregarAlquiler: unAlquiler.self assert: unCliente saldoAPagar equals: 10.! !!PruebasBikeRental methodsFor: 'tests' stamp: 'BrunoGrassano 6/27/2020 17:56'!test03AgregoDosAlquileresDevuelveTotal	|unCliente bici alquiler1 alquiler2|	bici := Bici conPrecio: 1.	alquiler1 := Alquiler crearAlquilerDiario: bici.	alquiler2 := Alquiler crearAlquilerPeriodo: bici paraPeriodo: 1.		unCliente := Cliente new. 	unCliente agregarAlquiler: alquiler1.	unCliente agregarAlquiler: alquiler2.		self assert: unCliente saldoAPagar equals:2! !!PruebasBikeRental methodsFor: 'tests' stamp: 'BrunoGrassano 6/27/2020 17:01'!test02AlquilerBiciPorPeriodoDe5| unaBici unAlquiler unCliente |unaBici := Bici conPrecio: 3.unAlquiler := Alquiler crearAlquilerPeriodo: unaBici paraPeriodo: 5.unCliente := Cliente new.unCliente agregarAlquiler: unAlquiler.self assert: unCliente saldoAPagar equals: 15.! !