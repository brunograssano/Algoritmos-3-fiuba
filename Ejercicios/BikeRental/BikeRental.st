Object subclass: #Alquiler	instanceVariableNames: 'bici tipo'	classVariableNames: ''	package: 'BikeRental'!!Alquiler methodsFor: 'as yet unclassified' stamp: 'BrunoGrassano 6/27/2020 16:40'!para: unaBici deTipo: tipoDiario.	bici := unaBici.	tipo := tipoDiario.! !!Alquiler methodsFor: 'as yet unclassified' stamp: 'BrunoGrassano 6/27/2020 16:52'!calcularCosto	^ tipo costoConBici: bici.! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Alquiler class	instanceVariableNames: ''!!Alquiler class methodsFor: 'as yet unclassified' stamp: 'BrunoGrassano 6/27/2020 16:39'!crearAlquilerDiario: unaBici 	|alquiler tipoDiario|	alquiler := self new.	tipoDiario := TipoDiario new.	alquiler para: unaBici deTipo: tipoDiario.	^alquiler! !!Alquiler class methodsFor: 'as yet unclassified' stamp: 'BrunoGrassano 6/27/2020 16:42'!crearAlquilerPeriodo: unaBici paraPeriodo: unPeriodo  	|alquiler tipoPeriodo|	alquiler := self new.	tipoPeriodo := TipoPeriodo conDias: unPeriodo .	alquiler para: unaBici deTipo: tipoPeriodo.	^alquiler! !Object subclass: #Bici	instanceVariableNames: 'precio'	classVariableNames: ''	package: 'BikeRental'!!Bici methodsFor: 'accessing' stamp: 'BrunoGrassano 6/27/2020 16:53'!precio	^precio! !!Bici methodsFor: 'as yet unclassified' stamp: 'BrunoGrassano 6/27/2020 16:49'!establecerPrecio: unPrecio	precio := unPrecio.! !!Bici methodsFor: 'as yet unclassified' stamp: 'BrunoGrassano 6/27/2020 16:55'!precioParaPeriodo: periodo	^precio * periodo! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Bici class	instanceVariableNames: ''!!Bici class methodsFor: 'as yet unclassified' stamp: 'BrunoGrassano 6/27/2020 17:01'!conPrecio: precio	|bici|	bici := self new.	bici establecerPrecio: precio.	^bici! !Object subclass: #Cliente	instanceVariableNames: 'alquileres'	classVariableNames: ''	package: 'BikeRental'!!Cliente methodsFor: 'as yet unclassified' stamp: 'BrunoGrassano 6/27/2020 17:01'!saldoAPagar	^alquileres sum: [ :alquiler | alquiler calcularCosto ].! !!Cliente methodsFor: 'as yet unclassified' stamp: 'BrunoGrassano 6/27/2020 16:58'!initialize	alquileres := OrderedCollection new.! !!Cliente methodsFor: 'as yet unclassified' stamp: 'PabloSuarez 6/29/2016 10:23'!agregarAlquiler: unAlquiler	alquileres add: unAlquiler .! !Object subclass: #TipoDiario	instanceVariableNames: ''	classVariableNames: ''	package: 'BikeRental'!!TipoDiario methodsFor: 'as yet unclassified' stamp: 'BrunoGrassano 6/27/2020 16:55'!costoConBici: bici	^bici precio.! !Object subclass: #TipoPeriodo	instanceVariableNames: 'periodo'	classVariableNames: ''	package: 'BikeRental'!!TipoPeriodo methodsFor: 'as yet unclassified' stamp: 'BrunoGrassano 6/27/2020 16:43'!agregarPeriodo: unPeriodo	periodo := unPeriodo! !!TipoPeriodo methodsFor: 'as yet unclassified' stamp: 'BrunoGrassano 6/27/2020 16:55'!costoConBici: bici	^bici precioParaPeriodo: periodo.! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!TipoPeriodo class	instanceVariableNames: ''!!TipoPeriodo class methodsFor: 'as yet unclassified' stamp: 'BrunoGrassano 6/27/2020 16:43'!conDias: unPeriodo	|tipoPeriodo|	tipoPeriodo := self new.	tipoPeriodo agregarPeriodo: unPeriodo.	^tipoPeriodo ! !