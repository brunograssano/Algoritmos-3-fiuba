Object subclass: #Club	instanceVariableNames: 'nombre tarifaSocio tarifaSocioVIP recaudacionTotal listaSocios'	classVariableNames: ''	package: 'TP - 0.5-TP'!!Club commentStamp: 'BrunoGrassano 4/29/2020 12:50' prior: 0!Instance Variables	listaSocios:		<Object>	nombre:		<Object>	recaudacionTotal:		<Object>	tarifaSocio:		<Object>	tarifaSocioVIP:		<Object>!!Club methodsFor: 'as yet unclassified' stamp: 'BrunoGrassano 4/29/2020 12:26'!recibirPagoDe: unNombre	| socioQuePaga |	socioQuePaga := self buscarSocio: unNombre.	recaudacionTotal := recaudacionTotal + socioQuePaga pagar! !!Club methodsFor: 'as yet unclassified' stamp: 'BrunoGrassano 4/29/2020 12:26'!cantidadPagadaPor: unNombre	| socioBuscado |	socioBuscado := self buscarSocio: unNombre.	^socioBuscado cantidadPagada! !!Club methodsFor: 'as yet unclassified' stamp: 'BrunoGrassano 4/29/2020 12:03'!agregarSocio: unSocio	listaSocios add: (unSocio agregarTarifa: tarifaSocio)! !!Club methodsFor: 'as yet unclassified' stamp: 'BrunoGrassano 4/27/2020 15:45'!establecerNombre: unNombre	nombre := unNombre! !!Club methodsFor: 'as yet unclassified' stamp: 'BrunoGrassano 4/29/2020 12:51'!nombresSocios	| nombresSocios |	nombresSocios := listaSocios collect: [ :socio | socio obtenerNombre ].	^nombresSocios.! !!Club methodsFor: 'as yet unclassified' stamp: 'BrunoGrassano 4/29/2020 12:09'!cantidadDeSocios	^listaSocios size! !!Club methodsFor: 'as yet unclassified' stamp: 'BrunoGrassano 4/27/2020 15:45'!establecerTarifaSocio: unaTarifa	tarifaSocio := unaTarifa! !!Club methodsFor: 'as yet unclassified' stamp: 'BrunoGrassano 4/27/2020 15:46'!establecerTarifaSocioVIP: unaTarifa	tarifaSocioVIP := unaTarifa ! !!Club methodsFor: 'as yet unclassified' stamp: 'BrunoGrassano 4/29/2020 12:03'!agregarSocioVIP: unSocio 	listaSocios add: (unSocio agregarTarifa: tarifaSocioVIP)! !!Club methodsFor: 'as yet unclassified' stamp: 'BrunoGrassano 4/29/2020 12:26'!buscarSocio: unNombre	^listaSocios detect:[ :socio | socio obtenerNombre == unNombre ]! !!Club methodsFor: 'accessing' stamp: 'BrunoGrassano 4/27/2020 15:48'!recaudacionTotal	^recaudacionTotal ! !!Club methodsFor: 'initialization' stamp: 'BrunoGrassano 4/29/2020 12:22'!initialize	recaudacionTotal := 0.	listaSocios := Set new.! !Object subclass: #Socio	instanceVariableNames: 'nombre tarifa vecesQuePago'	classVariableNames: ''	package: 'TP - 0.5-TP'!!Socio commentStamp: 'BrunoGrassano 4/29/2020 12:50' prior: 0!Internal Representation and Key Implementation Points.    Instance Variables	nombre:		<Object>	tarifa:		<Object>	vecesQuePago:		<Object>!!Socio methodsFor: 'as yet unclassified' stamp: 'BrunoGrassano 4/29/2020 12:03'!agregarTarifa: unaTarifa	tarifa := unaTarifa! !!Socio methodsFor: 'as yet unclassified' stamp: 'BrunoGrassano 4/27/2020 15:54'!establecerNombre: unNombre	nombre := unNombre! !!Socio methodsFor: 'as yet unclassified' stamp: 'BrunoGrassano 4/29/2020 12:19'!pagar	vecesQuePago := vecesQuePago + 1.	^tarifa! !!Socio methodsFor: 'as yet unclassified' stamp: 'BrunoGrassano 4/29/2020 11:55'!obtenerNombre	^nombre! !!Socio methodsFor: 'as yet unclassified' stamp: 'BrunoGrassano 4/29/2020 12:20'!cantidadPagada	^vecesQuePago * tarifa	! !!Socio methodsFor: 'initialization' stamp: 'BrunoGrassano 4/29/2020 12:19'!initialize 	vecesQuePago := 0! !