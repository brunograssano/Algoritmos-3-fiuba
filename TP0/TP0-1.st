Object subclass: #ComisionCurricularPermanente	instanceVariableNames: 'expedientesDeLaComision expedienteActual'	classVariableNames: ''	package: 'TP0-1'!!ComisionCurricularPermanente commentStamp: 'BrunoGrassano 4/18/2020 12:33' prior: 0!-!!ComisionCurricularPermanente methodsFor: 'initialization' stamp: 'BrunoGrassano 4/18/2020 11:59'!initialize	expedientesDeLaComision := OrderedCollection new.	expedienteActual := 1.! !!ComisionCurricularPermanente methodsFor: 'as yet unclassified' stamp: 'BrunoGrassano 4/18/2020 12:07'!votarEnContra| expedienteVotado |	expedienteVotado := expedientesDeLaComision at: expedienteActual.		expedienteVotado agregarVoto: 0! !!ComisionCurricularPermanente methodsFor: 'as yet unclassified' stamp: 'BrunoGrassano 4/18/2020 12:13'!procesarExpediente	| expedienteProcesado  |	expedienteProcesado := expedientesDeLaComision at: expedienteActual.	expedienteActual := expedienteActual + 1.		expedienteProcesado cambiarEstado.	^expedienteProcesado! !!ComisionCurricularPermanente methodsFor: 'as yet unclassified' stamp: 'BrunoGrassano 4/18/2020 11:52'!recibirExpediente: unExpediente	expedientesDeLaComision add: unExpediente ! !!ComisionCurricularPermanente methodsFor: 'as yet unclassified' stamp: 'BrunoGrassano 4/18/2020 12:21'!votarAFavor	| expedienteVotado |	expedienteVotado := expedientesDeLaComision at: expedienteActual.		expedienteVotado agregarVoto: 1	! !Object subclass: #Expediente	instanceVariableNames: 'nombreExpediente estadoExpediente votosAFavor votosEnContra'	classVariableNames: ''	package: 'TP0-1'!!Expediente commentStamp: 'BrunoGrassano 4/18/2020 12:32' prior: 0!-!!Expediente methodsFor: 'accessing' stamp: 'BrunoGrassano 4/18/2020 12:25'!cambiarEstado	estadoExpediente := votosAFavor > votosEnContra		ifTrue: [ 'Aprobado' ]		ifFalse: [ 'Rechazado' ]! !!Expediente methodsFor: 'initialization' stamp: 'BrunoGrassano 4/18/2020 12:19'!initialize	estadoExpediente := 'Iniciado'.	votosAFavor :=0.	votosEnContra :=0.! !!Expediente methodsFor: 'as yet unclassified' stamp: 'BrunoGrassano 4/18/2020 11:49'!establecerNombre: unNombre	nombreExpediente := unNombre! !!Expediente methodsFor: 'as yet unclassified' stamp: 'BrunoGrassano 4/18/2020 11:50'!estado	^estadoExpediente! !!Expediente methodsFor: 'as yet unclassified' stamp: 'BrunoGrassano 4/18/2020 12:19'!votosAFavor	^votosAFavor! !!Expediente methodsFor: 'as yet unclassified' stamp: 'BrunoGrassano 4/18/2020 12:19'!votosEnContra	^votosEnContra! !!Expediente methodsFor: 'as yet unclassified' stamp: 'BrunoGrassano 4/18/2020 12:22'!agregarVoto: unVoto	( unVoto == 1 ) ifTrue: [ 		votosAFavor := votosAFavor +1. 	]	ifFalse:[		votosEnContra := votosEnContra +1.	].! !