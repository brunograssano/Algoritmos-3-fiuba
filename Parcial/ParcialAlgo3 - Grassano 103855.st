Object subclass: #Averiado	instanceVariableNames: ''	classVariableNames: ''	package: 'ParcialAlgo3'!!Averiado methodsFor: 'as yet unclassified' stamp: 'BrunoGrassano 6/30/2020 20:21'!cantidadAAvanzarConVehiculo: vehiculo enTiempo: tiempo	^0! !Object subclass: #EstadoBueno	instanceVariableNames: ''	classVariableNames: ''	package: 'ParcialAlgo3'!!EstadoBueno methodsFor: 'as yet unclassified' stamp: 'BrunoGrassano 6/30/2020 20:57'!cantidadAAvanzarConVehiculo: vehiculo enTiempo: tiempo	vehiculo ifNil: [ VehiculoNoInicializadoError new signal ].	^vehiculo cuantoAvanzaEn:tiempo! !Object subclass: #MotoDeCarrera	instanceVariableNames: ''	classVariableNames: ''	package: 'ParcialAlgo3'!!MotoDeCarrera methodsFor: 'as yet unclassified' stamp: 'BrunoGrassano 6/30/2020 20:51'!cuantoAvanzaEn:tiempo	(tiempo<0)ifTrue:[TiempoInvalidoError new signal].	^240*tiempo! !Object subclass: #Motoneta	instanceVariableNames: ''	classVariableNames: ''	package: 'ParcialAlgo3'!!Motoneta methodsFor: 'as yet unclassified' stamp: 'BrunoGrassano 6/30/2020 20:51'!cuantoAvanzaEn:tiempo	(tiempo<0)ifTrue:[TiempoInvalidoError new signal].	^40*tiempo! !Error subclass: #TiempoInvalidoError	instanceVariableNames: ''	classVariableNames: ''	package: 'ParcialAlgo3'!Error subclass: #VehiculoNoInicializadoError	instanceVariableNames: ''	classVariableNames: ''	package: 'ParcialAlgo3'!Object subclass: #VehiculoTerrestre	instanceVariableNames: 'tipo estado'	classVariableNames: ''	package: 'ParcialAlgo3'!!VehiculoTerrestre methodsFor: 'initialization' stamp: 'BrunoGrassano 6/30/2020 20:33'!initialize	estado := EstadoBueno new.! !!VehiculoTerrestre methodsFor: 'as yet unclassified' stamp: 'BrunoGrassano 6/30/2020 19:30'!guardarTipo: unTipoDeVehiculo	tipo := unTipoDeVehiculo ! !!VehiculoTerrestre methodsFor: 'as yet unclassified' stamp: 'BrunoGrassano 6/30/2020 20:27'!romperVehiculo	estado := Averiado new.! !!VehiculoTerrestre methodsFor: 'accessing' stamp: 'BrunoGrassano 6/30/2020 20:57'!cantidadKmAlAvanzarHoras: cantidadDeHorasQueAvanza	^estado cantidadAAvanzarConVehiculo: tipo enTiempo: cantidadDeHorasQueAvanza.! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!VehiculoTerrestre class	instanceVariableNames: ''!!VehiculoTerrestre class methodsFor: 'as yet unclassified' stamp: 'BrunoGrassano 6/30/2020 19:32'!crearMotoDeCarrera	| vehiculoTerrestre unaMotoDeCarrera |	vehiculoTerrestre := self new.	unaMotoDeCarrera := MotoDeCarrera new.	vehiculoTerrestre guardarTipo: unaMotoDeCarrera.	^ vehiculoTerrestre! !!VehiculoTerrestre class methodsFor: 'as yet unclassified' stamp: 'BrunoGrassano 6/30/2020 19:33'!crearMotoneta	| unaMotoneta vehiculoTerrestre |	vehiculoTerrestre := self new.	unaMotoneta := Motoneta new.	vehiculoTerrestre guardarTipo: unaMotoneta.	^ vehiculoTerrestre ! !